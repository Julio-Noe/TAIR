        tair:AiaPrReq_9.4_3,
        tair:AiaPrReq_9.4_4,
        tair:AiaPrReq_9.4_5,
        tair:AiaPrReq_9.4_6,
        tair:AiaPrReq_9.5_1,
        tair:AiaPrReq_9.5_2,
        tair:AiaPrReq_9.6_1,
        tair:AiaPrReq_9.7_1,
        tair:AiaPrReq_9.7_2,
        tair:AiaPrReq_9.8_1,
        tair:AiaPrReq_9.9_1 ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_continuous_iterative_process ;
    tair:uses tair:le_risk_management_measures ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(2)(d)"@en ;
    skos:definition "[The continuous iterative process] shall [comprise] the following steps: adoption of suitable risk management measures in accordance with the provisions of the following paragraphs"@en .

tair:AiaPrReq_10.2_2 a tair:Requirement ;
    tair:decomposes tair:DataAndDataGovernanceReqColl ;
    tair:implementedBy tair:le_data_governance_practices,
        tair:le_data_management_practices ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 10(2)(a)"@en ;
    skos:definition "Those [data governance and management] [practices shall concern in particular] the relevant design choices"@en .

tair:AiaPrReq_10.2_3 a tair:Requirement ;
    tair:decomposes tair:DataAndDataGovernanceReqColl ;
    tair:implementedBy tair:le_data_governance_practices,
        tair:le_data_management_practices ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 10(2)(b)"@en ;
    skos:definition "Those [data governance and management] [practices shall concern in particular] data collection"@en .

tair:AiaPrReq_10.2_4 a tair:Requirement ;
    tair:affectedBy tair:AiaPrReq_10.2_1,
        tair:le_data_preparation_operations ;
    tair:decomposes tair:DataAndDataGovernanceReqColl ;
    tair:implementedBy tair:le_data_governance_practices,
        tair:le_data_management_practices ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 10(2)(c)"@en ;
    skos:definition "Those [data governance and management] [practices shall concern in particular] relevant data preparation processing operations, such as annotation, labelling, cleaning, enrichment and aggregation"@en .

tair:AiaPrReq_10.2_5 a tair:Requirement ;
    tair:decomposes tair:DataAndDataGovernanceReqColl ;
    tair:implementedBy tair:le_data_governance_practices,
        tair:le_data_management_practices ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 10(2)(d)"@en ;
    skos:definition "Those [data governance and management] [practices shall concern in particular] the formulation of relevant assumptions, notably with respect to the information that the data are supposed to measure and represent;"@en .

tair:AiaPrReq_10.2_6 a tair:Requirement ;
    tair:decomposes tair:DataAndDataGovernanceReqColl ;
    tair:implementedBy tair:le_data_governance_practices,
        tair:le_data_management_practices ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 10(2)(e)"@en ;
    skos:definition "Those [data governance and management] [practices shall concern in particular] a prior assessment of the availability, quantity and suitability of the data sets that are needed."@en .

tair:AiaPrReq_10.2_7 a tair:Requirement ;
    tair:affectedBy tair:AiaPrReq_10.2_1,
        tair:le_bias ;
    tair:decomposes tair:DataAndDataGovernanceReqColl ;
    tair:implementedBy tair:le_data_governance_practices,
        tair:le_data_management_practices ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 10(2)(f)"@en ;
    skos:definition "Those [data governance and management] [practices shall concern in particular] examination in view of possible biases"@en .

tair:AiaPrReq_17.1_a a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:Provider ;
    tair:uses tair:le_quality_management_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)"@en ;
    skos:broader tair:MssReq_4.1 ;
    skos:definition "Providers of high-risk AI systems shall put a quality management system in place that ensures compliance with this Regulation."@en .

tair:AiaPrReq_17.1_b a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)"@en ;
    skos:broader tair:CreatingAndUpdatingDocumentedInformationReqColl ;
    skos:definition "That [quality management] system shall be documented in a systematic and orderly manner in the form of written policies, procedures and instructions."@en .

tair:AiaPrReq_17.1_c_1 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    tair:uses tair:ConformityAssessment ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(a)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management] [shall include at least the following aspects] a strategy for regulatory compliance, including compliance with conformity assessment procedures and procedures for the management of modifications to the high-risk AI system"@en .

tair:AiaPrReq_17.1_c_10 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(j)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management system] [shall include at least the following aspects] the handling of communication with national competent authorities, competent authorities, including sectoral ones, providing or supporting the access to data, notified bodies, other operators, customers or other interested parties"@en .

tair:AiaPrReq_17.1_c_11 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    tair:uses tair:le_systems_and_procedures_for_record ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(k)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management system] [shall include at least the following aspects] systems and procedures for record keeping of all relevant documentation and information"@en .

tair:AiaPrReq_17.1_c_12 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    tair:uses tair:le_resource_management ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(l)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management system] [shall include at least the following aspects] resource management, including security of supply related measures"@en .

tair:AiaPrReq_17.1_c_2 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    tair:uses tair:le_design_control_of_the_high_risk_AI_system,
        tair:le_design_of_the_high_risk_AI_system,
        <http://openscience.adaptcentre.ie/ontologies/tair/le_design_verification_of_the_high_risk_AI_system.> ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(b)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management system] [shall include at least the following aspects] techniques, procedures and systematic actions to be used for the design, design control and design verification of the high-risk AI system"@en .

tair:AiaPrReq_17.1_c_3 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    tair:uses tair:le_quality_assurance_high-risk_AI_system,
        tair:le_quality_control_high-risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(c)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That quality management system] [shall include at least the following aspects] techniques, procedures and systematic actions to be used for the development, quality control and quality assurance of the high-risk AI system."@en .

tair:AiaPrReq_17.1_c_4 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    tair:uses tair:le_examination_procedure_of_the_high_risk_AI_system,
        tair:le_test_procedure_of_the_high_risk_AI_system,
        tair:le_validation_procedure_of_the_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(d)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management system] [shall include at least the following aspects] examination, test and validation procedures to be carried out before, during and after the development of the high-risk AI system, and the frequency with which they have to be carried out"@en .

tair:AiaPrReq_17.1_c_5 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(e)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management system] [shall include at least the following aspects] technical specifications, including standards, to be applied and, where the relevant harmonised standards are not applied in full, the means to be used to ensure that the high-risk AI system complies with the requirements set out in Chapter 2 of this Title"@en .

tair:AiaPrReq_17.1_c_6 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    tair:uses tair:le_systems_and_procedures_for_data_management ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(f)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management system] [shall include at least the following aspects] systems and procedures for data management, including data collection, data analysis, data labelling, data storage, data filtration, data mining, data aggregation, data retention and any other operation regarding the data that is performed before and for the purposes of the placing on the market or putting into service of high-risk AI systems;"@en .

tair:AiaPrReq_17.1_c_7 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    tair:uses tair:le_risk_management_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(g)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management system] [shall include at least the following aspects] the risk management system referred to in Article 9"@en .

tair:AiaPrReq_17.1_c_8 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    tair:uses tair:le_post_market_monitoring_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(h)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management system] [shall include at least the following aspects] the setting-up, implementation and maintenance of a post-market monitoring system, in accordance with Article 61"@en .

tair:AiaPrReq_17.1_c_9 a tair:Requirement ;
    tair:decomposes tair:QualityManagementSystemReqColl ;
    tair:implementedBy tair:le_quality_management_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 17(1)(i)"@en ;
    skos:broader tair:AimssConjReq_4.1_R1.1 ;
    skos:definition "That [quality management system] [shall include at least the following aspects] procedures related to the reporting of serious incidents and of malfunctioning in accordance with Article 62"@en .

tair:AiaPrReq_33_10_1 a tair:Requirement ;
    tair:decomposes tair:NotifiedBodiesReqColl ;
    tair:implementedBy tair:le_notified_bodies ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 33(10)"@en ;
    skos:definition "Notified bodies shall have sufficient internal competences to be able to effectively evaluate the tasks conducted by external parties on their behalf. "@en .

tair:AiaPrReq_33_10_2 a tair:Requirement ;
    tair:decomposes tair:NotifiedBodiesReqColl ;
    tair:implementedBy tair:le_notified_bodies ;
    tair:uses tair:le_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 33(10)"@en ;
    skos:definition "To that end, at all times and for each conformity assessment procedure and each type of high-risk AI system in relation to which they have been designated, the notified body shall have permanent availability of sufficient administrative, technical and scientific personnel who possess experience and knowledge relating to the relevant artificial intelligence technologies, data and data computing and to the requirements set out in Chapter 2 of this Title."@en .

tair:AiaPrReq_33_4_1 a tair:Requirement ;
    tair:decomposes tair:NotifiedBodiesReqColl ;
    tair:implementedBy tair:le_notified_bodies ;
    tair:uses tair:le_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 33(4)"@en ;
    skos:definition "Notified bodies shall be independent of the provider of a high-risk AI system in relation to which it performs conformity assessment activities. "@en .

tair:AiaPrReq_33_4_2 a tair:Requirement ;
    tair:decomposes tair:NotifiedBodiesReqColl ;
    tair:implementedBy tair:le_notified_bodies ;
    tair:uses tair:le_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 33(4)"@en ;
    skos:definition "Notified bodies shall also be independent of any other operator having an economic interest in the high-risk AI system that is assessed, as well as of any competitors of the provider."@en .

tair:AiaPrReq_33_9 a tair:Requirement ;
    tair:decomposes tair:NotifiedBodiesReqColl ;
    tair:implementedBy tair:le_notified_bodies ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 33(9)"@en ;
    skos:definition "Notified bodies shall be capable of carrying out all the tasks falling to them under this Regulation with the highest degree of professional integrity and the requisite competence in the specific field, whether those tasks are carried out by notified bodies themselves or on their behalf and under their responsibility."@en .

tair:AiaPrReq_9.3_1 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_risk_management_measures ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(3)"@en ;
    skos:definition "The risk management measures referred to in paragraph 2, point (d) shall give due consideration to the effects and possible interactions resulting from the combined application of the requirements set out in this Chapter 2."@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.3_2 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_risk_management_measures ;
    tair:uses tair:CommonSpecifications,
        tair:le_harmonised_standards ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(3)"@en ;
    skos:definition "They [risk management measures] shall take into account the generally acknowledged state of the art, including as reflected in relevant harmonised standards or common specifications."@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.4_1 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_risk_management_measures ;
    tair:uses tair:IntendedPurpose,
        tair:ReasonablyForseeableMisuse,
        tair:le_residual_risk ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(4)"@en ;
    skos:definition "The risk management measures referred to in paragraph 2, point (d) shall be such that any residual risk associated with each hazard as well as the overall residual risk of the high-risk AI systems is judged acceptable, provided that the high-risk AI system is used in accordance with its intended purpose or under conditions of reasonably foreseeable misuse."@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.4_2 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_residual_risk ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(4)"@en ;
    skos:definition "Those residual risks shall be communicated to the user."@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.4_3 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_risk_management_measures ;
    tair:uses tair:le_risk_management_measures ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(4)(a)"@en ;
    skos:definition "[In identifying the most appropriate risk management measures, the following shall be ensured:]  elimination or reduction of risks as far as possible through adequate design and development;"@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.4_4 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_risk_management_measures ;
    tair:uses tair:le_risk_management_measures ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(4)(b)"@en ;
    skos:definition "[In identifying the most appropriate risk management measures, the following shall be ensured:] where appropriate, implementation of adequate mitigation and control measures in relation to risks that cannot be eliminated;"@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.4_5 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_risk_management_measures ;
    tair:uses tair:le_risk_management_measures ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(4)(c)"@en ;
    skos:definition "[In identifying the most appropriate risk management measures, the following shall be ensured:] provision of adequate information pursuant to Article 13, in particular as regards the risks referred to in paragraph 2, point (b) of this Article, and, where appropriate, training to users."@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.4_6 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_eliminating_or_reducing_risks ;
    tair:uses tair:le_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(4)"@en ;
    skos:definition "In eliminating or reducing risks related to the use of the high-risk AI system, due consideration shall be given to the technical knowledge, experience, education, training to be expected by the user and the environment in which the system is intended to be used."@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.5_1 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(5)"@en ;
    skos:definition "High-risk AI systems shall be tested for the purposes of identifying the most appropriate risk management measures. "@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.5_2 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_testing_of_high_risk_AI_systems ;
    tair:uses tair:IntendedPurpose,
        tair:PerformanceOfAnAISystem,
        tair:le_perform_consistently_for_their_intended_purpose ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(5)"@en ;
    skos:definition "Testing shall ensure that high-risk AI systems perform consistently for their intended purpose and they are in compliance with the requirements set out in this Chapter."@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.6_1 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_testing_procedures ;
    tair:uses tair:IntendedPurpose ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(6)"@en ;
    skos:definition "Testing procedures shall be suitable to achieve the intended purpose of the AI system and do not need to go beyond what is necessary to achieve that purpose."@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.7_1 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_testing_of_high_risk_AI_systems ;
    tair:uses tair:le_development_process,
        tair:le_placing_on_the_market,
        tair:le_putting_into_service ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(7)"@en ;
    skos:definition "The testing of the high-risk AI systems shall be performed, as appropriate, at any point in time throughout the development process, and, in any event, prior to the placing on the market or the putting into service. "@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.7_2 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_testing_of_high_risk_AI_systems ;
    tair:uses tair:IntendedPurpose,
        tair:le_preliminarily_defined_metrics,
        tair:le_preliminarily_defined_probablistic_thresholds ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(7)"@en ;
    skos:definition "Testing shall be made against preliminarily defined metrics and probabilistic thresholds that are appropriate to the intended purpose of the high-risk AI system."@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.8_1 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_risk_management_measures ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(8)"@en ;
    skos:definition "When implementing the risk management system described in paragraphs 1 to 7, specific consideration shall be given to whether the high-risk AI system is likely to be accessed by or have an impact on children."@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_9.9_1 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_credit_institution ;
    tair:uses tair:le_risk_management_procedure ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(9)"@en ;
    skos:broader tair:MssReq_4.1 ;
    skos:definition "For credit institutions regulated by Directive 2013/36/EU, the aspects described in paragraphs 1 to 8 shall be part of the risk management procedures established by those institutions pursuant to Article 74 of that Directive."@en .

tair:AiaPrReq_10.2_8 a tair:Requirement ;
    tair:affectedBy tair:AiaPrReq_10.2_1,
        tair:le_data_gaps_or_shortcomings ;
    tair:decomposes tair:DataAndDataGovernanceReqColl ;
    tair:implementedBy tair:le_data_governance_practices,
        tair:le_data_management_practices ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 10(2)(g)"@en ;
    skos:definition "Those [data governance and management] [practices shall concern in particular] the identification of any possible data gaps or shortcomings, and how those gaps and shortcomings can be addressed."@en .

tair:AiaPrReq_13.2 a tair:Requirement ;
    tair:decomposes tair:TransparencyAndProvisionOfInformationToUsersReqColl ;
    tair:implementedBy tair:le_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 13(2)"@en ;
    skos:definition "High-risk AI systems shall be accompanied by instructions for use in an appropriate digital format or otherwise that include concise, complete, correct and clear information that is relevant, accessible and comprehensible to users."@en ;
    skos:related tair:MssConjReq_8.1_R2 .

tair:AiaPrReq_40.1 a tair:Requirement ;
    tair:decomposes tair:HarmonisedStandardsReqColl ;
    tair:implementedBy tair:le_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 40(1)"@en ;
    skos:definition "High-risk AI systems which are in conformity with harmonised standards or parts thereof the references of which have been published in the Official Journal of the European Union shall be presumed to be in conformity with the requirements set out in Chapter 2 of this Title, to the extent those standards cover those requirements."@en .

tair:AiaPrReq_41.1 a tair:Requirement ;
    tair:affectedBy tair:HarmonisedStandardsReqColl,
        tair:RequirementsForHighRiskAiSystemsReqColl ;
    tair:decomposes tair:CommonSpecificationsReqColl ;
    tair:implementedBy tair:le_the_commission ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 41(1)"@en ;
    skos:definition "Where harmonised standards referred to in Article 40 do not exist or where the Commission considers that the relevant harmonised standards are insufficient or that there is a need to address specific safety or fundamental right concerns, the Commission may, by means of implementing acts, adopt common specifications in respect of the requirements set out in Chapter 2 of this Title. Those implementing acts shall be adopted in accordance with the examination procedure referred to in Article 74(2)."@en .

tair:AiaPrReq_9.2_1 a tair:Requirement ;
    tair:decomposes tair:RiskManagementSystemReqColl ;
    tair:implementedBy tair:le_risk_management_system ;
    tair:uses tair:le_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 9(2)"@en ;
    skos:definition "The risk management system shall consist of a continuous iterative process run throughout the entire lifecycle of a high-risk AI system, requiring regular systematic updating. "@en ;
    skos:related tair:AimssReqSpec_6.1.1b .

tair:AiaPrReq_10.2_1 a tair:Requirement ;
    tair:decomposes tair:DataAndDataGovernanceReqColl ;
    tair:implementedBy tair:le_training_validation_and_testing_data_sets ;
    tair:uses tair:le_data_governance_practices,
        tair:le_data_management_practices ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 10(2)"@en ;
    skos:definition "Training, validation and testing data sets shall be subject to appropriate data governance and management practices."@en ;
    skos:related tair:MssConjReq_9.1_R1b .

tair:AiaPrReq_14.3_1 a tair:Requirement ;
    tair:decomposes tair:HumanOversightReqColl ;
    tair:implementedBy tair:le_human_oversight ;
    tair:uses tair:le_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 14(3)(a)"@en ;
    skos:definition "[Human oversight shall be ensured through either one or all of the following measures] identified and built, when technically feasible, into the high-risk AI system by the provider before it is placed on the market or put into service."@en ;
    skos:related tair:MssConjReq_5.3_R1,
        tair:MssConjReq_9.1_R1b .

tair:AiaPrReq_14.3_2 a tair:Requirement ;
    tair:affectedBy tair:Provider,
        tair:User ;
    tair:decomposes tair:HumanOversightReqColl ;
    tair:implementedBy tair:le_human_oversight ;
    tair:uses tair:le_high_risk_AI_system ;
    dct:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52021PC0206> ;
    skos:altLabel "Article 14(3)(b)"@en ;
    skos:definition "[Human oversight shall be ensured through either one or all of the following measures] identified by the provider before placing the high-risk AI system on the market or putting it into service and that are appropriate to be implemented by the user."@en ;
    skos:related tair:MssConjReq_5.3_R1,
        tair:MssConjReq_9.1_R1b .

